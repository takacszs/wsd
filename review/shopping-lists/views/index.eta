<% layout("./layouts/layout.eta") %>
<h1>Shared shopping lists</h1>
<a href="/lists">Lists</a>
<ul>
    <% if (it.listNum === 0) { %>
        <a>No shopping lists yet.</a>
    <% } else { %>
        <li>Shopping lists: <%= it.listNum %></li>
        <li>Shopping list items: <%= it.itemNum %></li>
    <% } %>
</ul>


